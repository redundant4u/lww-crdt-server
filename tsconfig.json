{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "./build",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "strict": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "paths": {}
    }
}
